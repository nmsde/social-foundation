{
	"name": "jimmitjoo/social-foundation",
    "authors": [
        {
            "name": "Jimmie Johansson",
            "email": "jimmitjoo@gmail.com",
            "homepage": "http://www.jimmitjoo.se",
            "role": "Developer"
        }
    ],
	"description": "Social Foundation is a Laravel based platform for social networking.",
	"keywords": ["social foundation", "social", "social network", "php social network", "php social platform", "social platform"],
	"license": "MIT",
    "version": "0.0.1",
    "type": "project",
    "time": "2014-08-26",
	"require": {
		"laravel/framework": "4.2.*",
		"laracasts/commander": "1.3.10",
		"laracasts/validation": "1.1.*",
		"laracasts/flash": "~1.0",
		"laracasts/presenter": "0.1.*"
	},
    "require-dev": {
        "way/generators": "2.6.1",
        "codeception/codeception": "2.0.*@dev",
        "laracasts/testdummy": "1.*",
        "fzaninotto/faker": "1.5.*@dev"
    },
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
        "psr-4": {
            "HACKson\\": "app/HACKson"
        }
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
